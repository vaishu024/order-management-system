# Ignore dependencies
node_modules/

# Ignore lock files (they change frequently)
package-lock.json
yarn.lock

# Ignore environment variables (contains sensitive credentials)
.env

# Ignore database backups
database-backup/

# Ignore logs and debug files
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore uploaded files (if any)
uploads/

# Ignore Postman collections and workspaces (optional)
.postman/

# Ignore OS-specific files (these are created automatically)
.DS_Store
Thumbs.db

# Ignore VS Code settings (optional)
.vscode/

# Ignore build files (if you have a build step)
dist/
build/

# Ignore Docker-related files (if using Docker)
docker-compose.override.yml
